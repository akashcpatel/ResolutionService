{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Warning",
      "UserService": "Information"
    }
  },
  "Publisher": {
    "UserChangedQueue": "user_changed_queue",
    "ResolutionChangedQueue": "resolution_changed_queue"
  },
  "Application": {
    "UserChangedReceiverServiceDelayInSeconds": "10"
  },
  "UserSearch": {
    "BaseUrl": "http://localhost:5000/",
    "FindUri":  "User/"
  },
  "Management": {
    "Endpoints": {
      "Health": {
        "ShowDetails": "whenauthorized",
        "Claim": {
          "Type": "health_actuator",
          "Value": "see_details"
        }
      }
    }
  }
}
